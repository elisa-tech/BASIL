[flake8]
max-line-length = 119
exclude =
  .git
  __pycache__
  .venv
  .idea
  */.DS_Store
  api/test/.pytest_cache
  examples
per-file-ignores =
    api/api.py:E402,E203
    api/testrun.py
    api/test/*.py:E402
    db/models/init_db.py:E402,F401
    api/tmttestrun.py:E402,F401
    db/models/test_run.py:F401
    db/models/test_run_config.py:F401
